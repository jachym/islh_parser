<xsl:stylesheet version="1.0"
xmlns:xsl="http://www.w3.org/1999/XSL/Transform"> 
<xsl:param name="ODD"/>
<xsl:param name="DIL"/>
<xsl:param name="POR"/>
<xsl:param name="PSK"/>
<xsl:template match='LHC'>
   <html>
      <body>
	 <p>
	    <xsl:value-of select='@LHC_KOD'/>
	    <xsl:apply-templates select="ODD[@ODD=$ODD]"/>
	 </p>
      </body>
   </html>
</xsl:template>
<xsl:template match="ODD">
   <div id="ODD">
      <xsl:value-of select='@ODD'/>
      <xsl:apply-templates select="DIL[@DIL=$DIL]"/>
   </div>
</xsl:template>
<xsl:template match="DIL">
   <div id="DIL">
      <xsl:value-of select='@DIL'/>
      <xsl:apply-templates select="POR[@POR=$POR]"/>
   </div>
</xsl:template>
<xsl:template match="POR">
   <div id="POR">
      <xsl:value-of select='@POR'/>
      <xsl:apply-templates select="PSK[@PSK=$PSK]"/>
   </div>
</xsl:template>
<xsl:template match="PSK">
   <div id="PSK">
      <xsl:value-of select='@PSK'/>
   </div>
</xsl:template>
</xsl:stylesheet>
